
.inpt-text{
	width: calc( 100% - 20px );
	margin-top: 10px;
	padding: 10px; 
	height: 300rpx;
	box-shadow: rgba(0, 0, 0, 0.16) 0px 1px 4px, rgb(51, 51, 51) 0px 0px 0px 3px;
	border-radius: 10px;
}
.inpt-sead{
	width: 100%;
	height: 100rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	background-color: #fccd43;
	border-radius: 10px;
}
.inpt-imge{
	margin: 100rpx 0;
}
.uni-file-picker{
}
.type-close{
	margin: 20px;
	
	display: flex;
	justify-content: center;
	padding: 10px;
	font-size: 16px;
	border-style: solid;
	border-color: black;
	border-width: 1px;
	border-radius: 10px;
}

